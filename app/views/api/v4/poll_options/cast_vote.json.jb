json = {}

json[:poll] = @poll.poll_options.map do |poll|
  Rails.cache.fetch ["v4", "poll", "show", @req_source, poll.id, poll.updated_at] do
    {
      id: poll.id,
      description: poll.description,
      numberOfVotes: poll.voters.count,
      voted: poll.voted?(current_user)
    }
  end
end

json