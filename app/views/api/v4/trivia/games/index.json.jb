json = { games: [] }
@games.each do |game|
  result = render partial: @req_source, locals: { game: game }, handler: :jb
  result[:user_enroled] = game.subscribers.first_or_initialize.persisted?
  result[:user_notification] = game.subscribers.first_or_initialize.subscribed?
  json[:games].push(result)
end

json
