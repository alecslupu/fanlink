{
  matches: @people.map do |person|
    person = Rails.cache.fetch(["v4", "person", "mini", p.id, p.updated_at.to_i]) do
      render partial: "api/v4/people/person_mini", locals: { person: person }, handlers: "jb"
    end
    r = Relationship.for_people(current_user, p.id)
    rel = render(partial: "api/v4/relationships/rel_mini", collection: r, as: :relationship)
    {
      person: person,
      relationship: rel.first,
      matched_interest_ids: p.matched_ids
    }
  end
}
