# frozen_string_literal: true
json = {}

json[:recommended_people] = @people.map do |person|
  Rails.cache.fetch current_user.cache_key_follow_person("v4", @req_source, current_user, person) do
    render partial: "api/v4/people/#{@req_source}", locals: { person: person }, handlers: "jb"
  end
end

json
