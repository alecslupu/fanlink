json = {}
if @posts
 json = render(partial: "api/v4/posts/rendered_posts", locals: { posts: @posts })
else
  json[:tags] = @tags.map do |tag|
    Rails.cache.fetch ["v4", "tag", @req_source, tag.id, tag.updated_at.to_i] do
      render partial: @req_source, locals: { tag: tag }, handlers: "jb"
    end
  end
end
json
