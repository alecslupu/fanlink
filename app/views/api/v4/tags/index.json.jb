json = {}
if @posts
  json[:posts] = @posts.map do |post|
    Rails.cache.fetch ["v4", @req_source, post.id, post.updated_at] do
      render partial: "api/v4/posts/#{@req_source}", locals: { post: post }, handlers: "jb"
    end
  end
else
  json[:tags] = @tags.map do |tag|
    Rails.cache.fetch ["v4", @req_source, tag.id, tag.updated_at] do
      render partial: @req_source, locals: { tag: tag }, handlers: "jb"
    end
  end
end
json
