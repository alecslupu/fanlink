# frozen_string_literal: true
json = {}

json[:post_reports] = @post_reports.map do |post_report|
  Rails.cache.fetch ["v4", "post_report", @req_source, post_report.id, post_report.updated_at.to_i] do
    render partial: @req_source, locals: { post_report: post_report }, handlers: "jb"
  end
end

json
