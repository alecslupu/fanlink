json = {}

json[:polls] = @polls.map do |poll|
  p = Rails.cache.fetch ["v5", "poll", @req_source, poll.id, poll.updated_at.to_i] do
    render partial: @req_source, locals: { poll: poll }, handlers: "jb"
  end
  if poll.poll_options.present?
    p[:options_count] = poll.poll_options.count if @req_source === 'web'
  end
  p
end

json
