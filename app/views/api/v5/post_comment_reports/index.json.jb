json[:post_comment_reports] = @post_comment_reports.map do |post_comment_report|
  Rails.cache.fetch ["v4", "comment_report", @req_source, post_comment_report.id, post_comment_report.updated_at.to_i] do
    render partial: @req_source, locals: { post_comment_report: post_comment_report }, handlers: "jb"
  end
end

json
