json = {}

json[:messages] = @messages.map do |message|
  m = Rails.cache.fetch ["v5", "message", @req_source, message.id, message.updated_at.to_i] do
    render partial: @req_source, locals: { message: message }, handlers: "jb"
  end
  if @req_source === "web"
    m[:person] = render partial: "person", locals: { person: message.person }, handlers: "jb"
    m[:picture_url] = message.picture_url
    m[:audio_url] = message.audio_url
  end
  m
end

json
