#!/bin/sh

# rm -f latest.dump*

# heroku pg:backups:capture --app fanlink-staging && heroku pg:backups:download --app fanlink-staging
# RAILS_ENV=development

docker exec -it flapi bin/rails db:environment:set db:force_drop && docker exec -i flapi-postgres pg_restore --verbose -c -O --no-acl -U postgres -h localhost -d fanlink_staging < ./latest.dump

docker exec -it flapi bin/rails db:migrate
